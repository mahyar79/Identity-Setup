@using NewIdentity.Resources
@model IEnumerable<NewIdentity.ViewModels.UserVM>

<h1>@Resource.All_Registered_Users</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>@Resource.UserName</th>
            <th>@Resource.First_Name</th>
            <th>@Resource.Family_Name</th>
            <th>@Resource.Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.UserName</td>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.Email</td>
            </tr>
        }
    </tbody>
</table>
